<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Kamon Medical</title>
    <!-- Custom CSS -->
    <link rel="stylesheet" href="style.css">
    <!-- Icons -->
    <link rel="stylesheet" href="https://maxst.icons8.com/vue-static/landings/line-awesome/line-awesome/1.3.0/css/line-awesome.min.css">
</head>
<body>
    <div class="container">
        <div class="container1">
            <div class="menu-bar">
                <div class="logo">
                <!-- Image here -->
                <img>
                </div>
                <ul>
                <li><a href="/">Home</a></li>
                <li><a href="/logout">Logout</a></li>
                </ul>
                <!-- Mobile Navigation Menu -->
              <div class="topnav">
                <!-- Navigation links (hidden by default) -->
                <div id="myLinks">
                  <a href="/">Home</a>
                </div>
                <!-- "Hamburger menu" / "Bar icon" to toggle the navigation links -->
                <a href="javascript:void(0);" class="icon" onclick="myFunction()">
                  <i class="las la-bars"></i>
                </a>
              </div>
            </div>
            <!-- Main Content -->
            <div class="intro">
                <div class="intro-text">
                    <p>Dashboard</p>
                </div>
                <div class="h-back">
                    <div class="arrow">
                        <img src="Images/arrow_back_ios_24px.png">
                    </div>
                    <div class="home">
                        <a href="/">Home</a>
                    </div>
                </div>
            </div>
            <div class="profile">
                <h2>Profile</h2>
            </div>
            <div class="p-hr"></div>
            <%if(locals.results){%>
              <% Object.values(results).forEach(function(user) {%>
            <div class="rect-1">
                <p><b>Name:</b><br><span><%=user.first_name%> <%=user.last_name%></span></p>
            </div>
            <div class="rect-2">
                <p><b>Email:</b><br><span><%=user.email%></span></p>
            </div>
            <div class="rect-3">
                <p><b>Number:</b><%=user.number%></p>
                <!-- <div class="change"><a href="#"></a></div> -->
            </div>
            <div class="rect-4">
                <p><b>Forum Username:</b><br><span><%=user.first_name%></span></p>
            </div>
            <div class="rect-5">
                <p><b>Medical Issues:</b><br><span>Focuses on improving returns on assets by investing in emerging cities and market in Africa</span></p>
            </div>
            <div class="rect-6">
                <a href="/visual">Check our <br>latest analysis</a>
            </div>
            <%})%>
            <%}%>
            <!-- End of main content -->
            <div class="download short">
                <div class="app">
                  <h2>Download our mobile app.</h2>
                </div>
                <div class="avail">
                  <h4>Available on your favorite store. Start your premium experience now</h4>
                </div>
                <div class="playstore">
                  <button>
                    <a href="#">Playstore</a>
                  </button>
                </div>
                <div class="appstore">
                  <button>
                    <a href="#">App store</a>
                  </button>
                </div>
              </div>
              <div class="foot footer">
                <div class="another-bg"></div>
                <div class="tweet">
                  <a href="#"><img src="Images/Vector-tweet.png"></a>
                </div>
                <div class="fb">
                  <a href="#"><img src="Images/Vector-fb.png"></a>
                </div>
                <div class="ig">
                  <a href="#"><img src="Images/Vector-ig.png" alt="IG"></a>
                </div>
                <p>&copy; Copywright 2020 HighQ Innovations</p>
              </div>
        </div>
    </div>
    <!-- Custom JS -->
    <script src="JS/bootstrap.min.js"></script>
</body>
</html>